num1=int(input())
num2=int(input())
operator=input()
if operator=='+':
    print("The Sum of",num1,"and",num2,"is",num1+num2)
elif operator=='-':
    print("The Difference of",num1,"and",num2,"is",num1-num2)
elif operator=='*':
    print("The Product of",num1,"and",num2,"is",num1*num2)
elif operator=='/':
    print("The Division of",num1,"and",num2,"is",num1/num2)
print()
